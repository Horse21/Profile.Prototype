{
	"compileOnSave": false,
	"compilerOptions": {
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"module": "es6",
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"target": "es5",
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2017",
			"dom"
		],
		"paths": {
			"@angular/*": [
				"../node_modules/@angular/*"
			],
			"rxjs/*": [
				"../node_modules/rxjs/*"
			],
			"jszip": [
				"../node_modules/jszip/dist/jszip.min.js"
			]
		}
	},
	"exclude": [
		"**/node_modules"
	]
}
